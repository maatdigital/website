.glarity--quota-insufficiently-container .glarity--quota-close {
  position: absolute;
  top: 24px;
  right: 24px;
  color: #2b3674;
  cursor: pointer;
  font-size: 18px;
}
.glarity--quota-insufficiently-container .glarity--quota-img {
  position: relative;
  display: flex;
  flex-direction: column;
}
.glarity--quota-insufficiently-container .glarity--quota-img .glarity--quota-img-text {
  width: fit-content;
  border-radius: 14px;
  background: #e0eeff;
  padding: 4px;
  box-sizing: border-box;
  color: #2b3674;
  font-size: 12px;
  margin: auto;
