  gap: 8px;
}
.glarity-chat-stroke_menuList .glarity-chat-stroke_menuList-item {
  display: flex;
  padding: 2px 8px;
  height: 28px;
  justify-content: center;
  align-items: center;
  gap: 4px;
  border-radius: 4px;
  background: #f2f6fa;
  box-sizing: border-box;
  gap: 4px;
  cursor: pointer;
}
.glarity-chat-stroke_menuList .glarity-chat-stroke_menuList-item:hover {
  color: #0080ff;
}
.glarity-chat-stroke_menuList .glarity-chat-stroke_menuList-item .glarity--selection-dropdown-text {
