---
title: "MAAT MAATgo"
layout: "data-spy"
fastspring: "maat-maatgo-perpetual-license"
---
<section id="home" class="dzsparallaxer auto-init dzsparallaxer---window-height use-loading" style="position: relative; height: 800px;" data-options='{  mode_scroll: "normal" }'>
  <div class="divimage dzsparallaxer--target " data-src="<%= url_for 'assets/images/bg1.jpg' %>" style="width: 100%; height: 130%; background-image: url()">
    <div class="hero-parallax">
      <div class="hero-inner">
        <div class="hero-content">
          <div class="container text-left">
            <div class="row">
              <div class="col-md-5 ml-auto mr-auto align-self-center">
                <div class="hero-static">
                  <h1>MAAT<em>go</em></h1>
                  <p class="lead">Take control of how you listen</p>

                  <div class="buttons">
                    <button data-fsc-item-path-value='<%= current_page.data.fastspring %>' data-fsc-action="Add, Checkout" class="btn btn-xl btn-primary"> Buy It Now <i class="ion-play"></i></button>
                    <div class="btn-group" role="group" aria-label="Button group with nested dropdown">
                      <button id="btnGroupDrop0" type="button" class="btn btn-white-omas-special-style-border btn-xl dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Try It Now
                      </button>
                      <div class="dropdown-menu" aria-labelledby="btnGroupDrop0">
                        <a href="https://www.maat.digital/dl/MAATgo-macos.zip" class="dropdown-item">Installer for macOS</a>
                        <a href="https://www.maat.digital/dl/MAATgo-windows.zip" class="dropdown-item">Installer for Windows</a>
                      </div>
                    </div>
                  </div>

                </div>
              </div>
              <div class="col-md-6">
                <%= image_tag "products/MAATgo/hero-maatgo.png", alt: "MAAT<em>go</em>", class: "img-fluid center-img" %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

